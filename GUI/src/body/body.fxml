<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.BorderPane?>

<TabPane fx:id="mainTabPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tabClosingPolicy="UNAVAILABLE" tabMinHeight="40.0" tabMinWidth="300.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="body.BodyController">
    <tabs>
        <Tab fx:id="flowLibTab" text="Library">
            <styleClass>
                <String fx:value="flowtab" />
                <String fx:value="clickable" />
            </styleClass>
            <content>
                <fx:include fx:id="flowLibComponent" source="library/libraryTab.fxml" />
            </content>
        </Tab>
      <Tab fx:id="flowExecTab" text="Execution">
        <content>
            <fx:include fx:id="flowExecComponent" source="execution/executionTab.fxml" />
        </content>
      </Tab>
      <Tab fx:id="flowHistoTab" text="History">
        <content>
            <fx:include fx:id="flowHistoryComponent" source="history/historyTab.fxml" />
        </content>
      </Tab>
      <Tab fx:id="flowStatTab" text="Statistics">
        <content>
            <fx:include fx:id="flowStatComponent" source="statistics/statisticsTab.fxml" />
        </content>
      </Tab>


        <!--include exeuctionTab-->
        <!-- include history-->
        <!-- include statistics-->
    </tabs>
</TabPane>
