<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="384.0" prefWidth="1250.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="body.library.input.InputController">
   <children>
      <Label fx:id="inputsLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="180.0" layoutY="14.0" prefHeight="48.0" prefWidth="288.0" text="Inputs" />
      <ScrollPane fx:id="inputsScrollPane" layoutX="66.0" layoutY="62.0" prefHeight="316.0" prefWidth="515.0">
         <content>
            <VBox fx:id="inputsVBox" prefHeight="314.0" prefWidth="513.0" />
         </content>
      </ScrollPane>
      <Pane fx:id="buttonWrapperForToolTip" layoutX="720.0" layoutY="177.0">
         <children>
            <Button fx:id="startFlowButton" ellipsisString="" mnemonicParsing="false" prefHeight="31.0" prefWidth="68.0">
               <tooltip>
                  <Tooltip fx:id="startFlowToolTip" text="In order to start fill atleast the mandatory inputs." />
               </tooltip>
            </Button>
         </children>
      </Pane>
      <Button fx:id="backToDefinitionButton" ellipsisString="" layoutX="720.0" layoutY="263.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="68.0">
         <tooltip>
            <Tooltip fx:id="executeFlowToolTip1" text="In order to start fill atleast the mandatory inputs." />
         </tooltip>
      </Button>
   </children>
</AnchorPane>
